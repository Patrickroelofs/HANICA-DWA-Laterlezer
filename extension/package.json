{
	"name": "extension",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.11.6",
		"@testing-library/react": "^11.1.2",
		"@testing-library/user-event": "^12.2.2",
		"node-fetch": "^2.6.1",
		"node-sass": "^4.0.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-loading": "^2.0.3",
		"react-scripts": "4.0.0",
		"web-vitals": "^0.2.4",
		"webpack-cli": "^4.2.0"
	},
	"watch": {
		"build": {
			"patterns": [
				"src/"
			],
			"extensions": ".js, .scss .jsx"
		}
	},
	"scripts": {
		"build:dev": "react-scripts build && webpack --mode development ./src/App.jsx -o ./build -w",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"watch": "npm-watch"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"npm-watch": "^0.7.0"
	}
}
